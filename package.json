{
  "name": "landing-page",
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "dev": "astro dev",
    "start": "astro dev",
    "build": "astro build",
    "preview": "astro preview",
    "astro": "astro",
    "lint": "prettier --write  \"**/*.{js,jsx,ts,tsx,md,mdx,astro}\" && eslint --fix \"src/**/*.{js,ts,jsx,tsx,astro}\" && astro build",
    "check-types": "tsc --pretty --noEmit",
    "check-format": "prettier --check . --log-level warn",
    "check-lint": "eslint . --ext ts --ext tsx --ext js --ext astro",
    "format": "prettier --write . --plugin=prettier-plugin-astro .",
    "test-all": "pnpm run check-format && pnpm run check-lint && pnpm run check-types && pnpm run build",
    "prepare": "husky install"
  },
  "dependencies": {
    "@astrojs/react": "^3.0.2",
    "@astrojs/tailwind": "^5.0.0",
    "@astrojs/ts-plugin": "^1.3.1",
    "@fontsource/lato": "^5.0.14",
    "@types/react-dom": "^18.0.6",
    "astro": "^3.2.0",
    "classnames": "^2.3.2",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "sass": "^1.68.0",
    "sharp": "^0.32.6",
    "tailwindcss": "^3.3.3"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@types/react": "^18.2.38",
    "@typescript-eslint/eslint-plugin": "^6.12.0",
    "@typescript-eslint/parser": "^6.12.0",
    "eslint": "^8.54.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-config-standard-with-typescript": "^39.1.1",
    "eslint-plugin-astro": "^0.29.1",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-n": "^16.3.1",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-plugin-promise": "^6.1.1",
    "husky": "^8.0.3",
    "prettier": "3.0.3",
    "prettier-config-standard": "^7.0.0",
    "prettier-plugin-astro": "^0.12.2",
    "prettier-plugin-tailwindcss": "^0.5.7",
    "typescript": "^5.3.2"
  }
}
