---
import Footer from '../components/Footer.astro';
import { Header } from '../components/Header';
import Courses from '../components/sections/courses/Courses.astro';
import GetInTouch from '../components/sections/getInTouch/GetInTouch.astro';
import OurGrads from '../components/sections/ourGrads/OurGrads.astro';
import Promo from '../components/sections/promo/Promo.astro';
import Questions from '../components/sections/questions/Questions.astro';
import SucessStories from '../components/sections/sucessStories/SucessStories.astro';
